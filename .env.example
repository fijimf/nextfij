# Docker Compose Environment Variables
# Copy this file to .env and update with your actual values

# Application Ports
UI_PORT=80
API_PORT=8073

# Spring Boot Configuration
SPRING_PROFILE=local

# Security Configuration (REQUIRED)
# Generate a secure JWT secret (minimum 32 characters)
JWT_SECRET=your-secure-jwt-secret-key-minimum-32-characters-long
# Set a strong admin password
ADMIN_PASSWORD=your-secure-admin-password

# Database Configuration
DB_NAME=deepfij_db
DB_USER=deepfij
# Generate a strong database password
DB_PASSWORD=your-secure-database-password

# SSL/Domain Configuration
# Your domain name for SSL certificate
DOMAIN_NAME=yourdomain.com
# Email for Let's Encrypt notifications
LETSENCRYPT_EMAIL=admin@yourdomain.com

# Example secure values (DO NOT USE IN PRODUCTION):
# JWT_SECRET=bXktc2VjcmV5NDMyZVM5bWItZGV2ZWxvcG1lbnQt
# ADMIN_PASSWORD=SecureAdmin2024!
# DB_PASSWORD=SecureDB2024!
# DOMAIN_NAME=example.com
# LETSENCRYPT_EMAIL=admin@example.com